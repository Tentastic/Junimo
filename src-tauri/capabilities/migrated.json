{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "local": true,
  "windows": [
    "main",
    "Config",
    "Profiles",
    "Exporter",
    "Importer"
  ],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "window:allow-start-dragging",
    "window:allow-close",
    "window:allow-minimize",
    "window:allow-toggle-maximize",
    "window:allow-internal-toggle-maximize",
    "app:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "dialog:allow-open",
    "fs:default",
    {
      "identifier": "fs:allow-read",
      "allow": [
        {
          "path": "$APPDATA/*"
        }
      ]
    }
  ]
}